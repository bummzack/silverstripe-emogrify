{
    "name": "bummzack/silverstripe-emogrify",
    "description": "Integrate Emogrifier plugin into SilverStripe.",
    "type" : "silverstripe-vendormodule",
    "license": "BSD-3-Clause",
    "keywords": [
        "silverstripe",
        "swiftmailer",
        "emogrifier"
    ],
    "require": {
        "silverstripe/recipe-core": "^1.1",
        "bummzack/swiftmailer-emogrifyplugin" : "^0.1"
    },
    "require-dev": {
        "phpunit/PHPUnit": "^5.7"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Bummzack\\SilverStripeEmogrify\\": "src/",
            "Bummzack\\SilverStripeEmogrify\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "lint": "phpcs -s src/ tests/"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

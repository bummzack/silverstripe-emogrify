{
    "name": "bummzack/silverstripe-emogrify",
    "description": "Integrate Emogrifier plugin into SilverStripe.",
    "type" : "silverstripe-vendormodule",
    "license": "BSD-3-Clause",
    "keywords": [
        "silverstripe",
        "swiftmailer",
        "emogrifier"
    ],
    "require": {
        "php": "^7.2 || ^8.0",
        "silverstripe/framework": "^4.9",
        "bummzack/swiftmailer-emogrifyplugin" : "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Bummzack\\SilverStripeEmogrify\\": "src/",
            "Bummzack\\SilverStripeEmogrify\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "lint": "vendor/bin/phpcs -s src/ tests/"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
